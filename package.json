{
  "name": "nexter",
  "version": "1.0.0",
  "description": "grid css consepts",
  "main": "index.js",
  "scripts": {
    "watch:sass": "node-sass sass/main.scss css/style.css --watch",
    "devserver": "live-server --browser=firefox",
    "start": "npm-run-all --parallel devserver watch:sass",
    "compile:sass": "node-sass sass/main.scss css/style.comp.css",
    "concat:css": "concat -o css/style.concat.css css/icon-font.css css/style.comp.css",
    "prefix:css": "postcss css/style.concat.css --use autoprefixer -b 'last 10 versions' -o css/style.prefix.css",
    "compress:css": "node-sass css/style.prefix.css css/style.css --output-style compressed",
    "build:css": "npm-run-all compile:sass concat:css prefix:css compress:css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/parul09singh/css-nexter.git"
  },
  "author": "parul",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/parul09singh/css-nexter/issues"
  },
  "homepage": "https://github.com/parul09singh/css-nexter#readme",
  "devDependencies": {
    "autoprefixer": "^9.8.6",
    "concat": "^1.0.3",
    "node-sass": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.7",
    "postcss-cli": "^8.3.1"
  }
}
